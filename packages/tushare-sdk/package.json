{
  "name": "@tushare/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Tushare Pro API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist .rslib node_modules"
  },
  "keywords": [
    "tushare",
    "finance",
    "stock",
    "typescript",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rslib/core": "^0.0.5",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.0.0",
    "jsdom": "^23.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/tushare-sdk.git",
    "directory": "packages/tushare-sdk"
  },
  "bugs": {
    "url": "https://github.com/your-org/tushare-sdk/issues"
  },
  "homepage": "https://github.com/your-org/tushare-sdk#readme"
}
