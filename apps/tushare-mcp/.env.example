# Tushare API Token (必填)
# 获取方式: https://tushare.pro/register
TUSHARE_TOKEN=your_32_character_token_here

# 日志级别 (可选,默认: info)
# 可选值: debug, info, warn, error
LOG_LEVEL=info

# 限流配置 (可选)
# 时间窗口内最大请求数
RATE_LIMIT_MAX_REQUESTS=100

# 限流时间窗口大小(毫秒)
RATE_LIMIT_WINDOW_MS=60000

# Tushare API 请求超时(毫秒)
REQUEST_TIMEOUT_MS=30000
