# Tushare API Token (必填)
# 获取方式: https://tushare.pro/register
TUSHARE_TOKEN=your_32_character_token_here

# 数据库路径
DATABASE_PATH=./data/stock.db

# 日志级别 (debug|info|warn|error)
LOG_LEVEL=info

# API 限流配置
# 并发请求数限制 (免费账户建议5)
RATE_LIMIT_CONCURRENT=5

# 限流后重试延迟 (毫秒)
RATE_LIMIT_RETRY_DELAY=60000

# Motia Workbench 端口
WORKBENCH_PORT=3000
