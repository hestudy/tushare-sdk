{
  "name": "tushare-sdk-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Tushare TypeScript SDK Monorepo",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepublish": "pnpm build && pnpm test && pnpm type-check",
    "publish:sdk": "cd packages/tushare-sdk && npm publish",
    "docs:dev": "pnpm --filter @tushare/docs dev",
    "docs:build": "pnpm --filter @tushare/docs build",
    "docs:preview": "pnpm --filter @tushare/docs preview",
    "docs:test": "pnpm --filter @tushare/docs test",
    "docs:test:e2e": "pnpm --filter @tushare/docs test:e2e"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
